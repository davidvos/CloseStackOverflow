{
    "name": "Close all Stack Overflow tabs",
    "version": "1.0",
    "description": "A simple click to close all Stack Overflow tabs you have currently open.",
    "browser_action": {
        "default_title": "Close all Stack Overflow tabs"
    },
    "permissions": [
        "tabs",
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" 
    },
    "manifest_version": 2
  }
